#include<stdio.h>
void main(){
    int n,m,i,j,s;
    int tab1[10],tab2[10];
    do {
        printf("donner n");
        scanf("%d",&n);
        printf("donner m");
        scanf("%d",&m);
    }while ((n<0) && (m<0));
    for (i=0;i<n;i++){
            printf("donner case tab[%d]:",i );
            scanf("%d",&tab1[i]);
        }
     for (j=0;j<m;j++){
            printf("donner case tab[%d]:",j );
            scanf("%d",&tab2[j]);
        }
    for (i=0;i<n;i++){
        printf("%d",tab1[i]);
        printf("\t");
    }
    printf("\n");
     for (j=0;j<m;j++){
        printf("%d",tab2[j]);
        printf("\t");
    }
    printf("\n");

    s=0;
    for (i=0;i<n;i++) {
        for (j=0;j<m;j++){
            s=s+tab1[i]*tab2[j];
        }

    }
     printf("le schtroumpt sera  = %d",s);



    }
